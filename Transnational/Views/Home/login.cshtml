                        alert(result.Message);

                    },

                    error: function (x, e) {
                        alert('Failed');
                        alert(x.responseText);
                        alert(x.status);

                    }
                });
            }

            $(document).ready(function () {
                $("#btnSave").click(function (e) {

                    AddEmp();
                    e.preventDefault();
                });
            });

