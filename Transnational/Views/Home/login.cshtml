        $(document).ready(function () {

            $.ajax({
                type: "GET",
                url: "/api/Country/GetAllCountry/Teamwork-SG-v2UAT/1",
                //data: { CD: 'Teamwork-SG-v2UAT', LanguageId: 1 },
                data: { CD: 'Teamwork_CRM_UAT', LanguageId: 1 },
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (data) {
                    var s = '<option value="-1">Please Select a Country </option>';

                    $.each(data.Result, function (index, val) {

                        s += '<option value="' + val.CountryId + '">' + val.CountryName + '</option>';
                        ///  s = s + '<option value="' + val.CountryId + '">' + val.CountryName + '</option>';
                    });
                    $("#CountryDropdown").html(s);
                }, //End of AJAX Success function

                failure: function (data) {

                    alert(data.responseText);

                }, //End of AJAX failure function
                error: function (data) {
                    alert(data.responseText);
                } //End of AJAX error function

            });
        });
